<%include ../partials/header.ejs%>

<div class="container">
    <div class="jumbotron">
        <h1>FAQ page</h1>
    </div>
    <%faqs.forEach(function(faq){%>
    	<a href="#">
			<div class="card">
            	<h3><%= faq.question %></h3>
            	<p><%=faq.answer.substring(0,100)%>...</p>
        	</div>
		</a>
    <% }) %>
</div>

<%include ../partials/footer.ejs%>